<head>
  
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">

  <script src="https://cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js"></script>
  <script type="text/javascript" src="/js/jquery-1.8.3.min.js"></script>

    <!-- Modernizr JS -->
  <script src="/js/modernizr-2.6.2.min.js"></script>

</head>

<body>
  <nav class="gtco-nav" role="navigation">
    <div class="gtco-container">
      <div class="row">
        <div class="col-sm-4 col-xs-12">
        <!-- <div class="col-md-4"> -->
          <div id="gtco-logo">
            <!-- <a href="#">
              <br><br>
              <img src="images/logo1.png">
            </a> -->
            <a href="/#">
                <script type="text/javascript" src="/js/mousedirection.js"></script>
    
                <img class="coin" id="coin" onmouseover="coinmove()">
                <script type="text/javascript" src="/js/coin.js"></script> 
            </a>
          </div>
        </div>

        <div class="col-md-8 text menu-1">
        <!-- <div class="col-xs-8 text-right menu-1"> -->
            <ul style="padding-top:20px;padding-left:10px">
              <li style="color:white;font-size:40px">Sucursal Virtual</li>
            </ul>
        </div>
      </div>
    </div>
  </nav>
  <center> 
    <div class="row" style="padding:450px">
      <div class="col-md-8">
        <div class="login">
          <header class="login-header"><span class="text">Ingresar</span><span class="loader"></span></header>

          <%= form_for(resource, as: resource_name, url: session_path(resource_name), class:"login-form") do |f| %>
            <div class="field">
              <%= f.email_field :email, autofocus: true, class:"login-input", placeholder: :email %>
            </div>

            <div class="field">
              <%= f.password_field :password, autocomplete: "off", class:"login-input", placeholder: :password %>
            </div>

            <% if devise_mapping.rememberable? -%>
              <div class="login-form">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me %>
              </div>
            <% end %>

            <div class="actions">
              <%= f.submit "Log in", class:"login-btn" %>
            </div>
          <% end %>

          <div class="login-form">
            <%= render "devise/shared/links" %>
          </div>
        </div>
      </div>
      <div class="col-md-4">Hola amigoss</div>
    </div>
  </center>
  <div class="lineBlack">
    <div class="gtco-container">
      
        <div class="gtco-footer">
          <p style="color:white;padding-top:20px;font-size:20px">&copy;Luis Díaz, Juan Gómez, Sorelys Sandoval</p>
        </div>
        
      
    </div>
  </div>
<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<!-- Waypoints -->
    <script src="/js/jquery.waypoints.min.js"></script>
    <!-- Carousel -->
    <!-- <script src="js/owl.carousel.min.js"></script>-->
    <!-- countTo -->
    <script src="/js/jquery.countTo.js"></script>
    <!-- Carousel -->
    <script src="/js/owl.carousel.min.js"></script>
    <!-- Magnific Popup -->
    <script src="/js/jquery.magnific-popup.min.js"></script>
    <script src="/js/magnific-popup-options.js"></script>
    <!-- Main -->
    <script src="/js/main.js"></script>
</body>
